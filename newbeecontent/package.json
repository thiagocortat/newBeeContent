  "private": true,
  "scripts": {
    "dev": "node scripts/setup-dev-schema.js && npx prisma generate && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "npx tsx prisma/seed.ts",
    "setup:dev": "node scripts/setup-environment.js dev",
